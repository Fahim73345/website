.facebook {
   display: block;
   float: right;
   margin-left: 3px;
   color: black;
   text-decoration: none;
   border-left: 1px solid gray;
   padding: 3px;
   border-right: 1px solid gray;
   cursor:pointer;


}

.facebook a {
   color: black;
}

.facebook a span {
   position: absolute;
   background: #0018A8;
   color: white;
   width: 100px;
   border-radius: 4px;
   font-size: 13px;
   display: block;
   text-align: center;
   font-weight: bold;
   font-style: arial black;
   top: 35px;

   visibility: hidden;
   opacity: 0;
   transition: 0.7s;

}

.facebook a span:before {
   content: "";
   border-left: 10px solid #0018A8;
   border-bottom: 10px solid #0018A8;
   position: absolute;
   left: 10px;
   bottom: 16px;
   transform: rotateZ(45deg);

}

.facebook a:hover span {
   visibility: visible;
   opacity: 1;

}

.facebook:active {
   color: #E14E17;
}

